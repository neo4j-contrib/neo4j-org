<%
    function findAttribute(attr,defaultValue) {
       var m = data.match(attr);
       return m ? m[1] : defaultValue;
    }
    var creator = item['twitter'] || findAttribute(/:twitter: *(.+)/,'neo4j');
    var gist_title = item['title'] || findAttribute(/^=+ *(.+)/,title||'A Neo4j Graph Gist');
    var gist_img = item['img'] || findAttribute(/image::(.+)\[/,asset('img/neo4j/neo4j-logo.png'));
